{
  "apiVersion": "vlabs",
  "properties": {
    "orchestratorProfile": {
      "OrchestratorVersion": "1.22.7",
      "kubernetesConfig": {
        "networkPlugin": "azure",
        "enableRbac": true,
        "addons": [
          {
            "name": "cluster-autoscaler",
            "enabled": true,
            "pools": [
              {
                "name": "agentpool1",
                "config": {
                  "min-nodes": "1",
                  "max-nodes": "10"
                }
              },
              {
                "name": "agentpool2",
                "config": {
                  "min-nodes": "1",
                  "max-nodes": "10"
                }
              }
            ],
            "config": {
              "scan-interval": "1m"
            }
          }
        ]
      }
    },
    "masterProfile": {
      "count": 3,
      "dnsPrefix": "",
      "vmSize": "Standard_D2_v3"
    },
    "agentPoolProfiles": [
      {
        "name": "agentpool1",
        "count": 1,
        "vmSize": "Standard_D2_v3"
      },
      {
        "name": "agentpool2",
        "count": 1,
        "vmSize": "Standard_D2_v3"
      }
    ],

    "linuxProfile": {
      "adminUsername": "azureuser",
      "ssh": {
        "publicKeys": [
          {
            "keyData": ""
          }
        ]
      }
    }
  }
}